INSERT INTO SCH_CENTRA_ID.DEFAULT_PROFILES (
                GLOBAL_PROFILE_ID, 
                CODE_COMPANY, 
                CODE_DIVISION, 
                CODE_AREA, 
                CODE_DEPARTMENT, 
                CODE_POSITION, 
                VALID, 
                NAME_COMPANY, 
                NAME_DIVISION, 
                NAME_AREA, 
                NAME_DEPARTMENT, 
                NAME_POSITION,
                AUTOMATIC
            ) 
            SELECT
                GLOBAL_PROFILE_ID,
                'BCP1',
                CASE WHEN CODE_DIVISION='ANY' THEN 'ANY' ELSE 'DD8888' END ,
                CASE WHEN CODE_AREA='ANY' THEN 'ANY' ELSE 'A8888' END,
                CASE WHEN CODE_DEPARTMENT='ANY' THEN 'ANY' ELSE 'EE8888' END ,
                CODE_POSITION,
                1,
                'BANCO PICHINCHA',
                CASE WHEN CODE_DIVISION='ANY' THEN '*' ELSE 'DIV 8888' END ,
                CASE WHEN CODE_AREA='ANY' THEN '*' ELSE 'ARE 8888' END,
                CASE WHEN CODE_DEPARTMENT='ANY' THEN '*' ELSE 'DTO 8888' END ,
                NAME_POSITION,
                1
            FROM (
            SELECT 
                GLOBAL_PROFILE_ID,
                CODE_COMPANY,
                CODE_DIVISION,
                CODE_AREA,
                CODE_DEPARTMENT,
                CODE_POSITION,
                NAME_POSITION 
            FROM SCH_CENTRA_ID.DEFAULT_PROFILES
            WHERE VALID=1
                AND (CODE_COMPANY='BCPI' )
                AND (CODE_DIVISION='N2001' OR CODE_DIVISION='ANY'  )
                AND (CODE_AREA='N3001' OR CODE_AREA='ANY'  )
                AND (CODE_DEPARTMENT='N40005' OR CODE_DEPARTMENT='ANY')
            ) t
            WHERE T.CODE_DIVISION!='ANY' OR T.CODE_AREA!='ANY' OR T.CODE_DEPARTMENT!='ANY'
            
