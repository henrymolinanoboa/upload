CREATE TABLE sch_sms.asignacion_usuarios (id BIGSERIAL NOT NULL, username CHARACTER VARYING(50) NOT NULL, seleccion BOOLEAN NOT NULL, asignacion_usuario_siguiente_id BIGINT NOT NULL, 
CONSTRAINT asignacion_usuarios_pk PRIMARY KEY (id));


