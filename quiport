 SELECT id,  fecha_hora,  numero,  nombre_tipo_notificacion,  fecha_creacion,  lugar,  nombre_evento_catalogo_nombre,  descripcion,  incidente_accidente_catalogo_nombre,  companias,  estado_nombre,  usuario_asignado FROM sch_sms.notificaciones_vw WHERE estado_nombre NOT LIKE 'ELIMINADO' AND codigo_tipo_notificacion LIKE '%VE'% AND estado_codigo LIKE '%ENELIM'% AND lugar_catalogo_codigo LIKE '%LUGEVE_LU67'% AND incidente_accidente_catalogo_codigo LIKE '%TIPACIN_EVE'% AND companias LIKE '%ABSA'% AND usuario_asignado LIKE '%hvacas'% AND mostrar_eliminados LIKE '%true'% AND DATE(fecha_creacion) BETWEEN  CURRENT_DATE  AND  TO_DATE(':today', 'YYYY-MM-DD') ORDER BY fecha_creacion desc LIMIT 10 OFFSET ( 1 - 1)*10
