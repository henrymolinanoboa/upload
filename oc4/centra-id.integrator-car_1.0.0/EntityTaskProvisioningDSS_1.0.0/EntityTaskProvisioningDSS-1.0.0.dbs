            <call-query href="queryOneToManyByMasterId__transactionalProfile_transactionalProfile_templateProfileMatrix">
                <with-param column="id" name="masterId"/>
            </call-query>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- DYNAMIC DISTINCT QUERY TASKPROVISIONING Application-->
    <query id="dynamicDistinctQuery_taskProvisioning_application" useConfig="default">
        <sql>
           SELECT DISTINCT
               application.id,
               application.code,
               application.name,
               application.automatic_provision,
               application.status,
               application.valid,
               application.application_group_id,
               application.parent_id
           FROM sch_centra_id.TAKS_PROVISIONING taskProvisioning WITH (NOLOCK) :innerjoinclause :whereclause ORDER BY :orderby
        </sql>
        <result element="applicationList" rowName="application">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="code" name="code" xsdType="xs:string"/>
            <element column="name" name="name" xsdType="xs:string"/>
            <element column="automatic_provision" name="automaticProvision" xsdType="xs:integer"/>
            <element column="status" name="status" xsdType="xs:string"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__application_application_applicationGroup">
                <with-param column="application_group_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__application_application_parent">
                <with-param column="parent_id" name="id"/>
            </call-query>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- INSERT TaskProvisioning -->
    <query id="insertQuery_taskProvisioning" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
        <sql>
           INSERT INTO sch_centra_id.TAKS_PROVISIONING(
               provisioning_date,
               provisioning_status,
               provisioning_comments,
               status,
               creation_date,
               uuid,
               uuid_parent,
               ticket_number,
               ticket_date,
               ticket_content,
               ticket_error,
               action,
               retry_number,
               type,
               retry_last_date,
               valid,
               transactional_profile_id,
               application_id,
               operation_provisioning_id
        ) VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="provisioningDate" ordinal="1" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="provisioningStatus" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="provisioningComments" ordinal="3" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="status" ordinal="4" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="A"/>
        <param name="creationDate" ordinal="5" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="uuid" ordinal="6" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="uuidParent" ordinal="7" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="ticketNumber" ordinal="8" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketDate" ordinal="9" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketContent" ordinal="10" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketError" ordinal="11" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="action" ordinal="12" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="retryNumber" ordinal="13" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
        <param name="type" ordinal="14" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="retryLastDate" ordinal="15" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="valid" ordinal="16" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
        <param name="transactionalProfileId" ordinal="17" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="applicationId" ordinal="18" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="operationProvisioningId" ordinal="19" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
    </query>
    <!-- UPDATE TaskProvisioning -->
    <query id="updateQuery_taskProvisioning" useConfig="default">
        <sql>
           UPDATE sch_centra_id.TAKS_PROVISIONING SET provisioning_date=?, provisioning_status=?, provisioning_comments=?, status=?, creation_date=?, uuid=?, uuid_parent=?, ticket_number=?, ticket_date=?, ticket_content=?, ticket_error=?, action=?, retry_number=?, type=?, retry_last_date=?, transactional_profile_id=?, application_id=?, operation_provisioning_id=?
            WHERE id=?
        </sql>
        <param name="provisioningDate" ordinal="1" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="provisioningStatus" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="provisioningComments" ordinal="3" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="status" ordinal="4" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="A"/>
        <param name="creationDate" ordinal="5" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="uuid" ordinal="6" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="uuidParent" ordinal="7" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="ticketNumber" ordinal="8" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketDate" ordinal="9" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketContent" ordinal="10" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="ticketError" ordinal="11" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="action" ordinal="12" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="retryNumber" ordinal="13" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
        <param name="type" ordinal="14" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="retryLastDate" ordinal="15" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="transactionalProfileId" ordinal="16" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="applicationId" ordinal="17" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="operationProvisioningId" ordinal="18" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="id" ordinal="19" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
    </query>
    <!-- QUERY REFERENCE TaskProvisioning TransactionalProfile 0-->
    <query id="queryReferenceById__taskProvisioning_transactionalProfile" useConfig="default">
        <sql>
           SELECT 
               transactionalProfile.id,
               transactionalProfile.code,
               transactionalProfile.name,
               transactionalProfile.comments,
               transactionalProfile.date_created,
               transactionalProfile.date_deleted,
               transactionalProfile.user_created,
               transactionalProfile.user_deleted,
               transactionalProfile.valid,
               transactionalProfile.configured,
               transactionalProfile.catalog_risk_level_id,
               transactionalProfile.application_id
           FROM sch_centra_id.TRANSACTIONAL_PROFILES transactionalProfile WITH (NOLOCK) WHERE id=? AND valid=1
        </sql>
        <result element="transactionalProfile">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="code" name="code" xsdType="xs:string"/>
            <element column="name" name="name" xsdType="xs:string"/>
            <element column="comments" name="comments" xsdType="xs:string"/>
            <element column="date_created" name="dateCreated" xsdType="xs:dateTime"/>
            <element column="date_deleted" name="dateDeleted" xsdType="xs:dateTime"/>
            <element column="user_created" name="userCreated" xsdType="xs:string"/>
            <element column="user_deleted" name="userDeleted" xsdType="xs:string"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
            <element column="configured" name="configured" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__transactionalProfile_transactionalProfile_catalogRiskLevel">
                <with-param column="catalog_risk_level_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__transactionalProfile_transactionalProfile_application">
                <with-param column="application_id" name="id"/>
            </call-query>
            <call-query href="queryOneToManyByMasterId__transactionalProfile_transactionalProfile_globalTransactionalProfile">
