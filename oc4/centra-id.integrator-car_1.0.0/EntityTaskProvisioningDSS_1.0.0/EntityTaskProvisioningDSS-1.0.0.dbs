<data name="provisioning.task-provisioning.ds" serviceNamespace="centra-id.com" enableBatchRequests="false" enableBoxcarring="false" transports="https" disableLegacyBoxcarringMode="true">

    <config id="default">
        <property name="carbon_datasource_name">CENTRA_ID</property>
    </config>
    <!-- TASKPROVISIONING LIST-->
    <query id="taskProvisioningList" useConfig="default">
        <sql>
            SELECT 
            ag.CODE APPLICATION_GROUP_CODE,
            ag.NAME APPLICATION_GROUP_NAME,
            a.CODE APPLICATION_CODE,
            a.NAME APPLICATION_NAME,
            a.TYPE_MANUAL_PROVISION APPLICATION_TYPE_MANUAL_PROVISION ,
            gp.CODE GLOBAL_PROFILE_CODE,
            gp.NAME GLOBAL_PROFILE_NAME,            
            tp.CODE TRANSACTIONAL_PROFILE_CODE,
            tp.NAME TRANSACTIONAL_PROFILE_NAME,
            ti.AREA_CODE,
            ti.AREA_NAME,
            ti.COMPANY_CODE,
            ti.COMPANY_NAME,
            ti.COST_CENTER_CODE,
            ti.COST_CENTER_NAME,
            ti.DEPARTMENT_CODE,
            ti.DEPARTMENT_NAME,
            ti.DEPOSIT_ACCOUNT_NUMBER,
            ti.DIVISION_CODE,
            ti.DIVISION_NAME,
            ti.EMPLOYEE_CODE,
            ti.EMPLOYEE_END_DATE,
            ti.EMPLOYEE_FIRSTNAME,
            ti.EMPLOYEE_LASTNAME,
            ti.EMPLOYEE_START_DATE,
            ti.EMPLOYEE_STATUS,
            ti.GLOBAL_PROFILE_ID,
            ti.GLOBAL_PROFILE_PREVIUS_ID,
            ti.GROUP_CODE,
            ti.GROUP_NAME,
            ti.HIERARCHICAL_LEVEL_CODE,
            ti.HIERARCHICAL_LEVEL_NAME,
            ti.IDENTIFICATION,
            ti.IDENTIFICATION_SUPERVISOR,
            ti.IDENTIFICATION_TYPE,
            ti.LOCATION_CODE,
            ti.LOCATION_NAME,
            ti.MAIL,
            ti.MOBILE,
            ti.MODIFY_DATA,
            ti.NAMES_SUPERVISOR,
            ti.ORIGEN,
            ti.PASSWORD,
            ti.POSITION_CODE,
            ti.POSITION_NAME,
            ti.PROFILE_RECALCULATION,
            ti.SUBSIDIARY_CODE,
            ti.SUBSIDIARY_NAME,
            ti.TIER,
            ti.TYPE_ACTION,
            ti.USER_SUBTYPE,
            ti.USER_TYPE,
            ti.USERNAME,
            ti.WORKER_CODE,
            ti.GUID,
            ti.UUID,
            tr.ACTION,
            tr.UUID TASK_UUID,
            tr.UUID_PARENT TASK_PARENT_UUID,
            ti.OBSERVATION GENERIC_OBSERVATION,
            ti.RESPONSIBLE_ADM GENERIC_RESPONSIBLE_ADM,
            ti.NO_TICKET GENERIC_NO_TICKET,
            ti.APPLICATION_CODE GENERIC_APPLICATION_CODE,
            ti.APPLICATION_NAME GENERIC_APPLICATION_NAME,
            ti.TECHNOLOGY_CODE GENERIC_TECHNOLOGY_CODE,
            ti.TECHNOLOGY_NAME GENERIC_TECHNOLOGY_NAME,
            ti.ENVIRONMENT_CODE GENERIC_ENVIRONMENT_CODE,
            ti.ENVIRONMENT_NAME GENERIC_ENVIRONMENT_NAME,
            ti.SERVER_NAME GENERIC_SERVER_NAME,
            ti.SERVER_IP GENERIC_SERVER_IP,
            ti.DATABASE_NAME GENERIC_DATABASE_NAME            
            FROM SCH_CENTRA_ID.TAKS_PROVISIONING tr WITH (NOLOCK)
            JOIN SCH_CENTRA_ID.OPERATIONS_PROVISIONING op WITH (NOLOCK) ON tr.OPERATION_PROVISIONING_ID=op.ID
            JOIN SCH_CENTRA_ID.TRANSACTIONAL_IDENTITY ti WITH (NOLOCK) ON op.TRANSACTIONAL_IDENTITY_ID=ti.ID
            JOIN SCH_CENTRA_ID.TRANSACTIONAL_PROFILES tp WITH (NOLOCK) ON tr.TRANSACTIONAL_PROFILE_ID=tp.ID
            JOIN SCH_CENTRA_ID.APPLICATIONS a WITH (NOLOCK) ON tp.application_id=a.ID
            JOIN SCH_CENTRA_ID.APPLICATIONS_GROUP ag WITH (NOLOCK) ON a.APPLICATION_GROUP_ID=ag.ID
            JOIN SCH_CENTRA_ID.GLOBAL_PROFILES gp WITH (NOLOCK) ON ti.GLOBAL_PROFILE_ID=gp.ID
            WHERE op.ID=? AND tr.TYPE=?
            ORDER BY tr.ACTION
        </sql>
        <result element="taskProvisioningList" rowName="taskProvisioning">
            <element column="APPLICATION_GROUP_CODE" name="applicationGroupCode" xsdType="xs:string"/>
            <element column="APPLICATION_GROUP_NAME" name="applicationGroupName" xsdType="xs:string"/>
            <element column="APPLICATION_CODE" name="applicationCode" xsdType="xs:string"/>
            <element column="APPLICATION_NAME" name="applicationName" xsdType="xs:string"/>
            <element column="APPLICATION_TYPE_MANUAL_PROVISION" name="applicationTypeManualProvision" xsdType="xs:string"/>
            <element column="GLOBAL_PROFILE_CODE" name="globalProfileCode" xsdType="xs:string"/>
            <element column="GLOBAL_PROFILE_NAME" name="globalProfileName" xsdType="xs:string"/>
            <element column="TRANSACTIONAL_PROFILE_CODE" name="transactionalProfileCode" xsdType="xs:string"/>
            <element column="TRANSACTIONAL_PROFILE_NAME" name="transactionalProfileName" xsdType="xs:string"/>
            <element column="AREA_CODE" name="areaCode" xsdType="xs:string"/>
            <element column="AREA_NAME" name="areaName" xsdType="xs:string"/>
            <element column="COMPANY_CODE" name="companyCode" xsdType="xs:string"/>
            <element column="COMPANY_NAME" name="companyName" xsdType="xs:string"/>
            <element column="COST_CENTER_CODE" name="costCenterCode" xsdType="xs:string"/>
            <element column="COST_CENTER_NAME" name="costCenterName" xsdType="xs:string"/>
            <element column="DEPARTMENT_CODE" name="departmentCode" xsdType="xs:string"/>
            <element column="DEPARTMENT_NAME" name="departmentName" xsdType="xs:string"/>
            <element column="DEPOSIT_ACCOUNT_NUMBER" name="depositAccountNumber" xsdType="xs:string"/>
            <element column="DIVISION_CODE" name="divisionCode" xsdType="xs:string"/>
            <element column="DIVISION_NAME" name="divisionName" xsdType="xs:string"/>
            <element column="EMPLOYEE_CODE" name="employeeCode" xsdType="xs:string"/>
            <element column="EMPLOYEE_END_DATE" name="employeeEndDate" xsdType="xs:date"/>
            <element column="EMPLOYEE_FIRSTNAME" name="employeeFirstname" xsdType="xs:string"/>
            <element column="EMPLOYEE_LASTNAME" name="employeeLastname" xsdType="xs:string"/>
            <element column="EMPLOYEE_START_DATE" name="employeeStartDate" xsdType="xs:date"/>
            <element column="EMPLOYEE_STATUS" name="employeeStatus" xsdType="xs:string"/>
            <element column="GLOBAL_PROFILE_ID" name="globalProfileId" xsdType="xs:integer"/>
            <element column="GLOBAL_PROFILE_PREVIUS_ID" name="globalProfilePreviusId" xsdType="xs:integer"/>
            <element column="GROUP_CODE" name="groupCode" xsdType="xs:string"/>
            <element column="GROUP_NAME" name="groupName" xsdType="xs:string"/>
            <element column="HIERARCHICAL_LEVEL_CODE" name="hierarchicalLevelCode" xsdType="xs:string"/>
            <element column="HIERARCHICAL_LEVEL_NAME" name="hierarchicalLevelName" xsdType="xs:string"/>
            <element column="IDENTIFICATION" name="identification" xsdType="xs:string"/>
            <element column="IDENTIFICATION_SUPERVISOR" name="identificationSupervisor" xsdType="xs:string"/>
            <element column="IDENTIFICATION_TYPE" name="identificationType" xsdType="xs:string"/>
            <element column="LOCATION_CODE" name="locationCode" xsdType="xs:string"/>
            <element column="LOCATION_NAME" name="locationName" xsdType="xs:string"/>
            <element column="MAIL" name="mail" xsdType="xs:string"/>
            <element column="MOBILE" name="mobile" xsdType="xs:string"/>
            <element column="MODIFY_DATA" name="modifyData" xsdType="xs:integer"/>
            <element column="NAMES_SUPERVISOR" name="namesSupervisor" xsdType="xs:string"/>
            <element column="ORIGEN" name="origen" xsdType="xs:string"/>
            <element column="PASSWORD" name="password" xsdType="xs:string"/>
            <element column="POSITION_CODE" name="positionCode" xsdType="xs:string"/>
            <element column="POSITION_NAME" name="positionName" xsdType="xs:string"/>
            <element column="PROFILE_RECALCULATION" name="profileRecalculation" xsdType="xs:integer"/>
            <element column="SUBSIDIARY_CODE" name="subsidiaryCode" xsdType="xs:string"/>
            <element column="SUBSIDIARY_NAME" name="subsidiaryName" xsdType="xs:string"/>
            <element column="TIER" name="tier" xsdType="xs:string"/>
            <element column="TYPE_ACTION" name="typeAction" xsdType="xs:string"/>
            <element column="USER_SUBTYPE" name="userSubtype" xsdType="xs:string"/>
            <element column="USER_TYPE" name="userType" xsdType="xs:string"/>
            <element column="USERNAME" name="username" xsdType="xs:string"/>
            <element column="WORKER_CODE" name="workerCode" xsdType="xs:string"/>
            <element column="GUID" name="guid" xsdType="xs:string"/>
            <element column="UUID" name="uuid" xsdType="xs:string"/>
            <element column="ACTION" name="action" xsdType="xs:string"/>
            <element column="TASK_UUID" name="taskUuid" xsdType="xs:string"/>
            <element column="TASK_PARENT_UUID" name="taskParentUuid" xsdType="xs:string"/>
            <element column="GENERIC_OBSERVATION" name="genericObservation" xsdType="xs:string"/>
            <element column="GENERIC_RESPONSIBLE_ADM" name="genericResponsibleAdm" xsdType="xs:integer"/>
            <element column="GENERIC_NO_TICKET" name="genericNoTicket" xsdType="xs:string"/>
            <element column="GENERIC_APPLICATION_CODE" name="genericApplicationCode" xsdType="xs:string"/>
            <element column="GENERIC_APPLICATION_NAME" name="genericApplicationName" xsdType="xs:string"/>
            <element column="GENERIC_TECHNOLOGY_CODE" name="genericTechnologyCode" xsdType="xs:string"/>
            <element column="GENERIC_TECHNOLOGY_NAME" name="genericTechnologyName" xsdType="xs:string"/>
            <element column="GENERIC_ENVIRONMENT_CODE" name="genericEnvironmentCode" xsdType="xs:string"/>
            <element column="GENERIC_ENVIRONMENT_NAME" name="genericEnvironmentName" xsdType="xs:string"/>
            <element column="GENERIC_SERVER_NAME" name="genericServerName" xsdType="xs:string"/>
            <element column="GENERIC_SERVER_IP" name="genericServerIp" xsdType="xs:string"/>
            <element column="GENERIC_DATABASE_NAME" name="genericDatabaseName" xsdType="xs:string"/>                  
        </result>
        <param name="operationProvisioningId" ordinal="1" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <param name="type" ordinal="2" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
    </query>





    <!-- DYNAMIC QUERY TASKPROVISIONING -->
    <query id="dynamicQuery_taskProvisioning" useConfig="default">
        <sql>
           SELECT
               taskProvisioning.id,
               taskProvisioning.provisioning_date,
               taskProvisioning.provisioning_status,
               taskProvisioning.provisioning_comments,
               taskProvisioning.status,
               taskProvisioning.creation_date,
               taskProvisioning.uuid,
               taskProvisioning.uuid_parent,
               taskProvisioning.ticket_number,
               taskProvisioning.ticket_date,
               taskProvisioning.ticket_content,
               taskProvisioning.ticket_error,
               taskProvisioning.action,
               taskProvisioning.retry_number,
               taskProvisioning.type,
               taskProvisioning.retry_last_date,
               taskProvisioning.valid,
               taskProvisioning.transactional_profile_id,
               taskProvisioning.application_id,
               taskProvisioning.operation_provisioning_id
           FROM sch_centra_id.TAKS_PROVISIONING taskProvisioning WITH (NOLOCK) :innerjoinclause :whereclause ORDER BY :orderby :fromto
        </sql>
        <result element="taskProvisioningList" rowName="taskProvisioning">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="provisioning_date" name="provisioningDate" xsdType="xs:date"/>
            <element column="provisioning_status" name="provisioningStatus" xsdType="xs:string"/>
            <element column="provisioning_comments" name="provisioningComments" xsdType="xs:string"/>
            <element column="status" name="status" xsdType="xs:string"/>
            <element column="creation_date" name="creationDate" xsdType="xs:date"/>
            <element column="uuid" name="uuid" xsdType="xs:string"/>
            <element column="uuid_parent" name="uuidParent" xsdType="xs:string"/>
            <element column="ticket_number" name="ticketNumber" xsdType="xs:string"/>
            <element column="ticket_date" name="ticketDate" xsdType="xs:date"/>
            <element column="ticket_content" name="ticketContent" xsdType="xs:string"/>
            <element column="ticket_error" name="ticketError" xsdType="xs:string"/>
            <element column="action" name="action" xsdType="xs:string"/>
            <element column="retry_number" name="retryNumber" xsdType="xs:integer"/>
            <element column="type" name="type" xsdType="xs:string"/>
            <element column="retry_last_date" name="retryLastDate" xsdType="xs:date"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__taskProvisioning_transactionalProfile">
                <with-param column="transactional_profile_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__taskProvisioning_application">
                <with-param column="application_id" name="id"/>
            </call-query>
            <call-query href="queryManyToOneById__taskProvisioning_operationProvisioning">
                <with-param column="operation_provisioning_id" name="id"/>
            </call-query>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="fromto" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- DYNAMIC DISTINCT QUERY TASKPROVISIONING TransactionalProfile-->
    <query id="dynamicDistinctQuery_taskProvisioning_transactionalProfile" useConfig="default">
        <sql>
           SELECT DISTINCT
               transactionalProfile.id,
               transactionalProfile.code,
               transactionalProfile.name,
               transactionalProfile.comments,
               transactionalProfile.date_created,
               transactionalProfile.date_deleted,
               transactionalProfile.user_created,
               transactionalProfile.user_deleted,
               transactionalProfile.valid,
               transactionalProfile.configured,
               transactionalProfile.catalog_risk_level_id,
               transactionalProfile.application_id,
           FROM sch_centra_id.TAKS_PROVISIONING taskProvisioning WITH (NOLOCK) :innerjoinclause :whereclause ORDER BY :orderby
        </sql>
        <result element="transactionalProfileList" rowName="transactionalProfile">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="code" name="code" xsdType="xs:string"/>
            <element column="name" name="name" xsdType="xs:string"/>
            <element column="comments" name="comments" xsdType="xs:string"/>
            <element column="date_created" name="dateCreated" xsdType="xs:dateTime"/>
            <element column="date_deleted" name="dateDeleted" xsdType="xs:dateTime"/>
            <element column="user_created" name="userCreated" xsdType="xs:string"/>
            <element column="user_deleted" name="userDeleted" xsdType="xs:string"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
            <element column="configured" name="configured" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__transactionalProfile_transactionalProfile_catalogRiskLevel">
                <with-param column="catalog_risk_level_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__transactionalProfile_transactionalProfile_application">
                <with-param column="application_id" name="id"/>
            </call-query>
            <call-query href="queryOneToManyByMasterId__transactionalProfile_transactionalProfile_globalTransactionalProfile">
                <with-param column="id" name="masterId"/>
            </call-query>
            <call-query href="queryOneToManyByMasterId__transactionalProfile_transactionalProfile_templateProfile">
                <with-param column="id" name="masterId"/>
