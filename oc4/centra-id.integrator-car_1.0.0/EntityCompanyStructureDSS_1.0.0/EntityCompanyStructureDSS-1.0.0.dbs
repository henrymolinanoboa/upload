                companyStructure.NAME_POSITION,
                companyStructure.FILTER
            FROM SCH_CENTRA_ID.COMPANIES_STRUCTURES_VW companyStructure
            WHERE ID_COMPANY=:idCompany AND CODE_DIVISION=:codeDivision AND (CODE_AREA=:codeArea OR CODE_AREA IS NULL) AND (CODE_DEPARTMENT=:codeDepartment OR CODE_DEPARTMENT IS NULL) AND NAME_POSITION=:namePosition
           
        </sql>
        <result element="companyStructureList" rowName="companyStructure">
            <element column="ID_COMPANY" name="idCompany" xsdType="xs:integer"/>
            <element column="CODE_COMPANY" name="codeCompany" xsdType="xs:string"/>
            <element column="NAME_COMPANY" name="nameCompany" xsdType="xs:string"/>
            <element column="TYPE_COMPANY" name="typeCompany" xsdType="xs:string"/>
            <element column="ID_DIVISION" name="idDivision" xsdType="xs:integer"/>
            <element column="CODE_DIVISION" name="codeDivision" xsdType="xs:string"/>
            <element column="NAME_DIVISION" name="nameDivision" xsdType="xs:string"/>
            <element column="ID_AREA" name="idArea" xsdType="xs:integer"/>
            <element column="CODE_AREA" name="codeArea" xsdType="xs:string"/>
            <element column="NAME_AREA" name="nameArea" xsdType="xs:string"/>
            <element column="ID_DEPARTMENT" name="idDepartment" xsdType="xs:integer"/>
            <element column="CODE_DEPARTMENT" name="codeDepartment" xsdType="xs:string"/>
            <element column="NAME_DEPARTMENT" name="nameDepartment" xsdType="xs:string"/>
            <element column="ID_POSITION" name="idPosition" xsdType="xs:integer"/>
            <element column="CODE_POSITION" name="codePosition" xsdType="xs:string"/>
            <element column="NAME_POSITION" name="namePosition" xsdType="xs:string"/>
            <element column="FILTER" name="filter" xsdType="xs:string"/>
        </result>
        <param name="idCompany" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="codeDivision" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="codeArea" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="codeDepartment" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="namePosition" paramType="SCALAR" sqlType="STRING" type="IN"/>
    </query>

    <!-- QUERY COMPANY JERARQUIA PLANA-->
    <query id="companyStructurePlaneList" useConfig="default">
        <sql>
            SELECT 
                companyStructure.ID_COMPANY,
                companyStructure.CODE_COMPANY,
                companyStructure.NAME_COMPANY,
                companyStructure.TYPE_COMPANY,
                companyStructure.ID_DIVISION,
                companyStructure.CODE_DIVISION,
                companyStructure.NAME_DIVISION,
                companyStructure.ID_AREA,
                companyStructure.CODE_AREA,
                companyStructure.NAME_AREA,
                companyStructure.ID_DEPARTMENT,
                companyStructure.CODE_DEPARTMENT,
                companyStructure.NAME_DEPARTMENT,
                companyStructure.ID_POSITION,
                companyStructure.CODE_POSITION,
                companyStructure.NAME_POSITION,
                companyStructure.FILTER
            FROM SCH_CENTRA_ID.COMPANIES_STRUCTURES_VW companyStructure
            WHERE FILTER LIKE :filter AND companyStructure.ID_COMPANY = :companyId
            ORDER BY NAME_DIVISION,NAME_AREA,NAME_DEPARTMENT,NAME_POSITION  
            OFFSET :count ROWS FETCH NEXT :startIndex ROWS ONLY
        </sql>
        <result element="companyStructureList" rowName="companyStructure">
            <element column="ID_COMPANY" name="idCompany" xsdType="xs:integer"/>
            <element column="CODE_COMPANY" name="codeCompany" xsdType="xs:string"/>
            <element column="NAME_COMPANY" name="nameCompany" xsdType="xs:string"/>
            <element column="TYPE_COMPANY" name="typeCompany" xsdType="xs:string"/>
            <element column="ID_DIVISION" name="idDivision" xsdType="xs:integer"/>
            <element column="CODE_DIVISION" name="codeDivision" xsdType="xs:string"/>
            <element column="NAME_DIVISION" name="nameDivision" xsdType="xs:string"/>
            <element column="ID_AREA" name="idArea" xsdType="xs:integer"/>
            <element column="CODE_AREA" name="codeArea" xsdType="xs:string"/>
            <element column="NAME_AREA" name="nameArea" xsdType="xs:string"/>
            <element column="ID_DEPARTMENT" name="idDepartment" xsdType="xs:integer"/>
            <element column="CODE_DEPARTMENT" name="codeDepartment" xsdType="xs:string"/>
            <element column="NAME_DEPARTMENT" name="nameDepartment" xsdType="xs:string"/>
            <element column="ID_POSITION" name="idPosition" xsdType="xs:integer"/>
            <element column="CODE_POSITION" name="codePosition" xsdType="xs:string"/>
            <element column="NAME_POSITION" name="namePosition" xsdType="xs:string"/>
            <element column="FILTER" name="filter" xsdType="xs:string"/>
        </result>
        <param name="filter" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="companyId" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <param name="startIndex" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <param name="count" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
    </query>

    <!-- QUERY COMPANY JERARQUIA PLANA COUNT-->
    <query id="companyStructurePlaneCount" useConfig="default">
        <sql>
            SELECT 
               COUNT(*) AS VALUE
            FROM SCH_CENTRA_ID.COMPANIES_STRUCTURES_VW companyStructure
            WHERE companyStructure.FILTER LIKE :filter AND companyStructure.ID_COMPANY = :companyId
        </sql>
        <result element="result">
            <element column="VALUE" name="value" xsdType="xs:long"/>
        </result>
        <param name="filter" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="companyId" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
    </query>



    <!-- DYNAMIC QUERY COMPANYSTRUCTURE -->
    <query id="dynamicQuery_companyStructure" useConfig="default">
        <sql>
           SELECT
               companyStructure.id,
               companyStructure.company_id,
               companyStructure.catalog_organizational_structure_id,
               companyStructure.parent_id,
               companyStructure.valid
           FROM sch_centra_id.COMPANIES_STRUCTURES companyStructure :innerjoinclause :whereclause ORDER BY :orderby :fromto
        </sql>
        <result element="companyStructureList" rowName="companyStructure">
            <element column="id" name="id" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__companyStructure_company">
                <with-param column="company_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__companyStructure_catalogOrganizationalStructure">
                <with-param column="catalog_organizational_structure_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__companyStructure_parent">
                <with-param column="parent_id" name="id"/>
            </call-query>
            <element column="valid" name="valid" xsdType="xs:integer"/>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="fromto" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- DYNAMIC DISTINCT QUERY COMPANYSTRUCTURE Company-->
    <query id="dynamicDistinctQuery_companyStructure_company" useConfig="default">
