           INSERT INTO sch_centra_id.COMPANIES_STRUCTURES(
               company_id,
               catalog_organizational_structure_id,
               parent_id,
               valid
        ) VALUES(?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="companyId" ordinal="1" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="catalogOrganizationalStructureId" ordinal="2" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="parentId" ordinal="3" paramType="SCALAR" sqlType="BIGINT" type="IN" defaultValue="#{NULL}"/>
        <param name="valid" ordinal="4" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
    </query>
    <!-- UPDATE CompanyStructure -->
