    <!-- INSERT CompanyStructure plane -->
    <query id="insertQuery_companyStructurePlane" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
        <sql>
            INSERT INTO SCH_CENTRA_ID.COMPANIES_STRUCTURES
                (
                    COMPANY_ID,
                    CATALOG_ORGANIZATIONAL_STRUCTURE_ID,
                    parent_id,
                    VALID,
                    COMPANY_CODE,
                    CATALOG_CODE,
                    CATALOG_TYPE_CODE
                )
                VALUES
                (
                    ?,
                    ?,
                    ?,
                    1,
                    '',
                    '',
                    ''
                )
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="companyId" ordinal="1" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="catalogId" ordinal="2" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="parentId" ordinal="3" paramType="SCALAR" sqlType="BIGINT" type="IN" defaultValue="#{NULL}"/>
    </query>
    <!-- INSERT CompanyStructure -->
    <query id="insertQuery_companyStructure" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
