    <query id="select_with_by_username_users_query" useConfig="default">
        <sql>
			 select a.username from (
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
				 union all
				 select ? as username
			 ) a 
			 WHERE a.username NOT IN ( select USERNAME as username from sch_centra_id.USERDATA  WITH (NOLOCK) ) 
        </sql>
        <param name="username1" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
