    <query id="update_all_query" useConfig="default" returnUpdatedRowCount="true">
        <sql>
           UPDATE :table SET :set WHERE :where
        </sql>
        <result element="UpdatedRowCount" rowName="" useColumnNumbers="true">
            <element column="1" name="Value" xsdType="integer" />
        </result>
        <param name="table" sqlType="QUERY_STRING" />
        <param name="set" sqlType="QUERY_STRING" />
        <param name="where" sqlType="QUERY_STRING" />
    </query>
    <query id="select_count_user_query" useConfig="default">
        <sql>
            SELECT COUNT(*) AS COUNT FROM SCH_CENTRA_ID.USERDATA WITH (NOLOCK) WHERE EMPLOYEE_CODE=?
        </sql>
        <param name="employeeCode" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
        <result element="Users" rowName="User">
            <element column="COUNT" name="count" xsdType="xs:long" />
        </result>
    </query>
    <query id="select_transacionational_profile_user_query" useConfig="default">
        <sql>
            SELECT TP.ID, TP.CODE,TP.NAME
            FROM SCH_CENTRA_ID.TRANSACTIONAL_PROFILES tp WITH (NOLOCK) 
            JOIN SCH_CENTRA_ID.APPLICATIONS a WITH (NOLOCK) ON tp.application_id=a.ID
            JOIN SCH_CENTRA_ID.GLOBAL_TRANSACTIONAL_PROFILES gtp WITH (NOLOCK) ON gtp.TRANSACTIONAL_PROFILE_ID=tp.ID
            JOIN SCH_CENTRA_ID.GLOBAL_PROFILES gp WITH (NOLOCK) ON gtp.GLOBAL_PROFILE_ID = gp.ID
            JOIN SCH_CENTRA_ID.USERDATA u WITH (NOLOCK) ON u.GLOBAL_PROFILE_ID=gp.ID
            WHERE gtp.VALID=1  AND u.USERNAME=?  AND a.CODE=?
        </sql>
        <param name="username" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
        <param name="application" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN" />
        <result element="TransactionalProfileList" rowName="TransactionalProfile">
            <element column="ID" name="id" xsdType="xs:integer" />
            <element column="CODE" name="code" xsdType="xs:string" />
            <element column="NAME" name="name" xsdType="xs:string" />
        </result>
    </query>
    <query id="select_expire_user_query" useConfig="default">
        <sql>
            SELECT ID
            FROM  SCH_CENTRA_ID.USERDATA WITH (NOLOCK) 
            WHERE EMPLOYEE_STATUS='ACTIVO' AND VALID=1 AND CAST(EXPIRATION_DATE_USER AS DATE) &lt;= CAST(GETDATE() AS DATE)
        </sql>
        <result element="UserDataList" rowName="UserData">
            <element column="ID" name="id" xsdType="xs:integer" />
        </result>
    </query>
    <query id="select_expire_global_profile_query" useConfig="default">
        <sql>
            SELECT ID,GLOBAL_PROFILE_ID,GLOBAL_PROFILE_ORIGINAL_ID
            FROM  SCH_CENTRA_ID.USERDATA WITH (NOLOCK) 
            WHERE EMPLOYEE_STATUS='ACTIVO' AND VALID=1 AND CAST(EXPIRATION_DATE_GLOBAL_PROFILE AS DATE) &lt;= CAST(GETDATE() AS DATE) AND
            AND GLOBAL_PROFILE_ORIGINAL_ID IS NOT NULL AND GLOBAL_PROFILE_ID &lt;> GLOBAL_PROFILE_ORIGINAL_ID
        </sql>
        <result element="UserDataList" rowName="UserData">
            <element column="ID" name="id" xsdType="xs:integer" />
            <element column="GLOBAL_PROFILE_ID" name="globalProfileId" xsdType="xs:integer" />
            <element column="GLOBAL_PROFILE_ORIGINAL_ID" name="globalProfileOriginalId" xsdType="xs:integer" />
        </result>
    </query>
    <query id="select_tasks_with_operation_query" useConfig="default">
        <sql>
            SELECT
                tp.ID ID,
                tp.UUID UUID,
                tp2.UUID UUID_PARENT
            FROM  SCH_CENTRA_ID.TAKS_PROVISIONING tp WITH (NOLOCK) 
            JOIN  SCH_CENTRA_ID.APPLICATIONS a WITH (NOLOCK) ON tp.APPLICATION_ID=a.ID
            LEFT JOIN SCH_CENTRA_ID.TAKS_PROVISIONING tp2 WITH (NOLOCK) ON tp2.OPERATION_PROVISIONING_ID=? AND tp2.APPLICATION_ID=a.PARENT_ID
            WHERE tp.ID=? AND tp.OPERATION_PROVISIONING_ID=?
        </sql>
        <param name="operationProvisioningId" ordinal="1" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <param name="taskProvisioningId" ordinal="2" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <param name="operationProvisioning2Id" ordinal="3" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <result element="TaskProvisioning" rowName="User">
            <element column="ID" name="id" xsdType="xs:integer" />
            <element column="UUID" name="uuid" xsdType="xs:string" />
            <element column="UUID_PARENT" name="uuidParent" xsdType="xs:string" />
        </result>
    </query>
    <query id="select_with_all_full_users_query" useConfig="default">
        <sql>
        SELECT
            USERNAME,
            CODIGO_EMPLEADO,
            CODIGO_TRABAJADOR,
            IDENTIFICACION,
            APELLIDOS_EMPLEADO,
            NOMBRES_EMPLEADO,
            CELULAR,
            NUMERO_CUENTA_DEPOSITO,
            FECHA_INGRESO,
            FECHA_SALIDA,
            IDENTIFICACION_SUPERVISOR,
            NOMBRE_SUPERVISOR,
            CODIGO_COMPANIA,
            NOMBRE_COMPANIA,
            CODIGO_DIVISION,
            NOMBRE_DIVISION,
            CODIGO_AREA,
            NOMBRE_AREA,
            CODIGO_DEPARTAMENTO,
            NOMBRE_DEPARTAMENTO,
            NOMBRE_CARGO,
            CODIGO_CENTRO_COSTO,
            NOMBRE_CENTRO_COSTO,
            CODIGO_AGRUPACION,
            CODIGO_NIVEL_JERARQUICO,
            TIPOUSUARIO,
            SUBTIPO,
            ESTADO_EMPLEADO,
            NOMBRELOCALIDAD,
            CORREO_ELECTRONICO,
            ID_PERFIL_GLOBAL,
            CODIGO_PERFIL_GLOBAL,
            NOMBRE_PERFIL_GLOBAL,
            ID_USUARIO
        FROM sch_centra_id.USERDATA_VW WITH (NOLOCK) 
        ORDER BY APELLIDOS_EMPLEADO,NOMBRES_EMPLEADO
        OFFSET ? ROWS
