    <query id="select_with_name_and_company_and_hierarchy_users_query" useConfig="default">
        <sql>
        SELECT 
            t1.username USERNAME,
            t1.EMPLOYEE_CODE CODIGO_EMPLEADO,
            ISNULL(t1.EMPLOYEE_FIRST_SURNAME,'') + ISNULL(' ' + t1.EMPLOYEE_SECOND_SURNAME,'') APELLIDOS_EMPLEADO,
            ISNULL(t1.EMPLOYEE_FIRST_NAME,'') + ISNULL(' ' + EMPLOYEE_SECOND_NAME,'') + ' - ' + t2.CODE NOMBRES_EMPLEADO,
            t1.ID ID_USUARIO
        FROM SCH_CENTRA_ID.USERDATA t1 
                join SCH_CENTRA_ID.COMPANIES t2 ON  t1.COMPANY_ID = t2.ID
                join SCH_CENTRA_ID.CATALOGS t3 ON t2.CATALOG_COMPANY_TYPE_ID=t3.ID
        WHERE  
                UPPER( t1.EMPLOYEE_COMPLETE_NAME) LIKE UPPER(?) AND 
                t1.EMPLOYEE_STATUS='ACTIVO' AND
                cast(t1.HIERARCHICAL_LEVEL as decimal(10,6)) &lt;= cast(? as decimal(10,6)) AND
                t3.CODE IN ('BAN','FIL')
        ORDER BY t1.EMPLOYEE_COMPLETE_NAME
        </sql>
        <param name="name" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
        <param name="hierarchyCode" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN" />
        <result element="Users" rowName="User">
            <element column="USERNAME" name="username" xsdType="xs:string" />
            <element column="CODIGO_EMPLEADO" name="codigoEmpleado" xsdType="xs:string" />
            <element column="APELLIDOS_EMPLEADO" name="apellidosEmpleado" xsdType="xs:string" />
