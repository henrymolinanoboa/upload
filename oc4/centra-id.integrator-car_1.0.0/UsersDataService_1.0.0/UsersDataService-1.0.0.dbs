<data name="UsersDSS" serviceNamespace="centra-id.com" transports="https">
    <config id="default">
        <property name="carbon_datasource_name">CENTRA_ID</property>
    </config>

    <query id="select_count_user_without_teller_number" useConfig="default">
        <sql>
                SELECT
                    COUNT(*) COUNT 
                FROM SCH_CENTRA_ID.GLOBAL_PROFILES GP 
                    JOIN SCH_CENTRA_ID.GLOBAL_TRANSACTIONAL_PROFILES GTP ON GTP.GLOBAL_PROFILE_ID=GP.ID
                    JOIN SCH_CENTRA_ID.TRANSACTIONAL_PROFILES TP ON GTP.TRANSACTIONAL_PROFILE_ID= TP.ID
                    JOIN SCH_CENTRA_ID.APPLICATIONS A ON TP.APPLICATION_ID=A.ID
                    JOIN SCH_CENTRA_ID.USERDATA UD ON UD.GLOBAL_PROFILE_ID=GP.ID
                WHERE
                    GTP.VALID=1 AND TP.VALID=1 AND A.CODE='BANCS' AND
                    UD.EMPLOYEE_STATUS='ACTIVO' AND
                    UD.VALID = 1 AND
                    UD.FILTER LIKE ? AND
                    UD.USERNAME NOT IN (
                    SELECT TELLER_NAME FROM SCH_CENTRA_ID.REST_USER_STORE_SCIM
                    WHERE VALID=1 AND REST_USER_STORE_SCIM.DOMAIN='BANCS' 
                    )
            </sql>
        <param name="filter" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
        <result element="UserList">
            <element column="COUNT" name="count" xsdType="xs:long" />
        </result>
    </query>

    <query id="select_list_user_without_teller_number" useConfig="default">
        <sql>
                SELECT
                    UD.ID USERDATA_ID,
                    UD.USERNAME USERDATA_USERNAME,
                    UD.EMPLOYEE_COMPLETE_NAME USERDATE_NAME ,
                    GP.ID GLOBAL_PROFILE_ID,
                    GP.CODE GLOBAL_PROFILE_CODE,
                    GP.NAME GLOBAL_PROFILE_NAME,
                    TP.ID TRANSACTIONAL_ID,
                    TP.CODE TRANSACTIONAL_CODE,
                    TP.NAME TRANSACTIONAL_NAME,
                    UD.FILTER
                FROM SCH_CENTRA_ID.GLOBAL_PROFILES GP 
                    JOIN SCH_CENTRA_ID.GLOBAL_TRANSACTIONAL_PROFILES GTP ON GTP.GLOBAL_PROFILE_ID=GP.ID
                    JOIN SCH_CENTRA_ID.TRANSACTIONAL_PROFILES TP ON GTP.TRANSACTIONAL_PROFILE_ID= TP.ID
                    JOIN SCH_CENTRA_ID.APPLICATIONS A ON TP.APPLICATION_ID=A.ID
                    JOIN SCH_CENTRA_ID.USERDATA UD ON UD.GLOBAL_PROFILE_ID=GP.ID
                WHERE
                    GTP.VALID=1 AND TP.VALID=1 AND A.CODE='BANCS' AND
                    UD.EMPLOYEE_STATUS='ACTIVO' AND
                    UD.VALID = 1 AND
                    UD.FILTER LIKE ? AND
                    UD.USERNAME NOT IN (
                    SELECT TELLER_NAME FROM SCH_CENTRA_ID.REST_USER_STORE_SCIM
                    WHERE VALID=1 AND REST_USER_STORE_SCIM.DOMAIN='BANCS' 
                    )
                    ORDER BY UD.EMPLOYEE_COMPLETE_NAME
                    OFFSET ? ROWS FETCH NEXT ? ROWS ONLY
            </sql>
        <param name="filter" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN" />
        <param name="startIndex" ordinal="2" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <param name="count" ordinal="3" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <result element="UserList" rowName="User">
            <element column="USERDATA_ID" name="userdataId" xsdType="xs:integer" />
            <element column="USERDATA_USERNAME" name="userdataUsername" xsdType="xs:string" />
            <element column="USERDATE_NAME" name="userdateName" xsdType="xs:string" />
            <element column="GLOBAL_PROFILE_ID" name="globalProfileId" xsdType="xs:integer" />
            <element column="GLOBAL_PROFILE_CODE" name="globalProfileCode" xsdType="xs:string" />
            <element column="GLOBAL_PROFILE_NAME" name="globalProfileName" xsdType="xs:string" />
            <element column="TRANSACTIONAL_ID" name="transactionalId" xsdType="xs:integer" />
            <element column="TRANSACTIONAL_CODE" name="transactionalCode" xsdType="xs:string" />
            <element column="TRANSACTIONAL_NAME" name="transactionalName" xsdType="xs:string" />
            <element column="FILTER" name="filter" xsdType="xs:string" />
        </result>
    </query>

    <query id="select_user_activate_mail" useConfig="default">
        <sql>
                SELECT 
                    ISNULL(T5.ACTIVATE_MAIL,0) ACTIVATE_MAIL,
                    ISNULL(T5.ACTIVATE_MAIL_CLOUD,0) ACTIVATE_MAIL_CLOUD 
                FROM SCH_CENTRA_ID.USERDATA T1
                JOIN SCH_CENTRA_ID.GLOBAL_TRANSACTIONAL_PROFILES T2 ON T1.GLOBAL_PROFILE_ID = T2.GLOBAL_PROFILE_ID
                JOIN SCH_CENTRA_ID.TRANSACTIONAL_PROFILES T3 ON T2.TRANSACTIONAL_PROFILE_ID=T3.ID
                JOIN SCH_CENTRA_ID.APPLICATIONS T4 ON T3.APPLICATION_ID = T4.ID
                JOIN SCH_CENTRA_ID.TEMPLATE_PROFILES_AD T5 ON T5.TRANSACTIONAL_PROFILE_ID=T3.ID
                WHERE T1.ID=? AND T4.CODE='AD' AND T2.VALID=1
            </sql>
        <param name="userdataId" ordinal="1" paramType="SCALAR" sqlType="INTEGER" type="IN" />
        <result element="User">
            <element column="ACTIVATE_MAIL" name="activateMail" xsdType="xs:string" />
            <element column="ACTIVATE_MAIL_CLOUD" name="activateMailCloud" xsdType="xs:string" />
        </result>
    </query>

    <query id="select_user_actions_massive_process_query" useConfig="default">
        <sql>
            SELECT DISTINCT
                'Accion' Accion,
                'uid' uid,
                '%FULL_NAME%' FULL_NAME,
                'bpApPaterno' bpApPaterno,
                'bpApMaterno' bpApMaterno,
                '%FIRST_NAME%' FIRST_NAME,
                'bpIdentificacion' bpIdentificacion,
                'bpTipoIdentificacion' bpTipoIdentificacion,
                'bpSupervisor' bpSupervisor,
                'bpCustom01' bpCustom01,
                'bpUnidad' bpUnidad,
                'bpArea' bpArea,
                'bpDepartamento'bpDepartamento,
                'bpCargo' bpCargo,
                'bpCentroCostos' bpCentroCostos,
                'bpTipoCuentaTALLA' bpTipoCuentaTALLA,
                'bpNumeroCuentaTALLA' bpNumeroCuentaTALLA,
                'bpInicioVigencia' bpInicioVigencia,
                'bpFinVigencia' bpFinVigencia,
                'bpMotivoSuspension' bpMotivoSuspension,
                'bpInicioSuspension' bpInicioSuspension,
                'bpFinSuspension' bpFinSuspension
            UNION ALL
            SELECT 
            --Accion
                CASE 
                WHEN a.TYPE_ACTION='C' THEN 'CREATE'
                WHEN a.TYPE_ACTION='U' THEN 'MODIFY'
                WHEN a.TYPE_ACTION='D' THEN 'DELETE'
                END Accion,
            --uid
                ISNULL(a.USERNAME,'') uid,
            --%FULL_NAME%
                ISNULL(b.EMPLOYEE_COMPLETE_NAME,'') FULL_NAME,
            --bpApPaterno
                ISNULL(b.EMPLOYEE_FIRST_SURNAME,'') bpApPaterno,
            --bpApMaterno
                ISNULL(b.EMPLOYEE_SECOND_SURNAME,'') bpApMaterno,
            --FIRST_NAME
                ISNULL(b.EMPLOYEE_FIRST_NAME,'') + ' ' + ISNULL(b.EMPLOYEE_SECOND_NAME,'') FIRST_NAME,
            -- bpIdentificacion
                ISNULL(a.IDENTIFICACION,'') bpIdentificacion,
            --bpTipoIdentificacion
                CASE 
                WHEN a.TIPO_IDENTIFICACION='C' THEN '0001'
                WHEN a.TIPO_IDENTIFICACION='P' THEN '0002'  
                END bpTipoIdentificacion,
            -- bpSupervisor
                CASE 
                WHEN c.USERNAME IS NOT NULL THEN 'uid='+c.USERNAME+',ou=Usuarios,o=Bpichincha'
                WHEN c.USERNAME IS NULL THEN ''
                END bpSupervisor,
            -- bpCustom01
                ISNULL(a.CODIGO_COMPANIA,'') bpCustom01,
            --bpUnidad
                ISNULL(a.NOMBRE_DIVISION,'') bpUnidad,
            -- bpArea
                ISNULL(a.NOMBRE_AREA,'') bpArea,
            --bpDepartamento
                ISNULL(a.NOMBRE_DEPARTAMENTO,'') bpDepartamento,
            -- bpCargo
                ISNULL(a.NOMBRE_CARGO,'') bpCargo,
            -- bpCentroCostos
                ISNULL(a.CODIGO_CENTRO_COSTO,'') bpCentroCostos,
            -- bpTipoCuentaTALLA
                '00' bpTipoCuentaTALLA,
            -- bpNumeroCuentaTALLA 
                ISNULL(a.NUMERO_CUENTA_DEPOSITO,'')  bpNumeroCuentaTALLA,
            -- bpInicioVigencia
                ISNULL(FORMAT(b.REGISTRATION_DATE,'dd-MM-yyyy'),'') bpInicioVigencia,
            -- bpFinVigencia
                ISNULL(FORMAT(b.ENDING_WORK_DATE,'dd-MM-yyyy'),'') bpFinVigencia,
            -- bpMotivoSuspension
                CASE WHEN a.TYPE_ACTION='D'THEN 'ELIMINAR' ELSE 'A'END  bpMotivoSuspension,
                '' bpInicioSuspension,
                '' bpFinSuspension
            FROM SCH_CENTRA_ID.MASSIVE_PROCESS a 
            LEFT JOIN SCH_CENTRA_ID.USERDATA b ON a.USERNAME=b.USERNAME
            LEFT JOIN SCH_CENTRA_ID.USERDATA c ON a.IDENTIFICACION_SUPERVISOR=c.IDENTIFICATION
            WHERE a.TYPE_EVENT='C' AND a.CODIGO_COMPANIA != 'TCS'
            and cast(a.FECHA_REGISTRO as date) = cast(getDate() as date)
        </sql>
        <result element="UserList" rowName="User">
            <element column="Accion" name="Accion" xsdType="xs:string" />
            <element column="uid" name="uid" xsdType="xs:string" />
            <element column="FULL_NAME" name="FULL_NAME" xsdType="xs:string" />
            <element column="bpApPaterno" name="bpApPaterno" xsdType="xs:string" />
            <element column="bpApMaterno" name="bpApMaterno" xsdType="xs:string" />
            <element column="FIRST_NAME" name="FIRST_NAME" xsdType="xs:string" />
            <element column="bpIdentificacion" name="bpIdentificacion" xsdType="xs:string" />
            <element column="bpTipoIdentificacion" name="bpTipoIdentificacion" xsdType="xs:string" />
            <element column="bpSupervisor" name="bpSupervisor" xsdType="xs:string" />
            <element column="bpCustom01" name="bpCustom01" xsdType="xs:string" />
            <element column="bpUnidad" name="bpUnidad" xsdType="xs:string" />
            <element column="bpArea" name="bpArea" xsdType="xs:string" />
            <element column="bpDepartamento" name="bpDepartamento" xsdType="xs:string" />
            <element column="bpCargo" name="bpCargo" xsdType="xs:string" />
            <element column="bpCentroCostos" name="bpCentroCostos" xsdType="xs:string" />
            <element column="bpTipoCuentaTALLA" name="bpTipoCuentaTALLA" xsdType="xs:string" />
            <element column="bpNumeroCuentaTALLA" name="bpNumeroCuentaTALLA" xsdType="xs:string" />
            <element column="bpInicioVigencia" name="bpInicioVigencia" xsdType="xs:string" />
            <element column="bpFinVigencia" name="bpFinVigencia" xsdType="xs:string" />
            <element column="bpMotivoSuspension" name="bpMotivoSuspension" xsdType="xs:string" />
            <element column="bpInicioSuspension" name="bpInicioSuspension" xsdType="xs:string" />
            <element column="bpFinSuspension" name="bpFinSuspension" xsdType="xs:string" />
        </result>
    </query>
