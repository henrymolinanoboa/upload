            UPDATE SCH_CENTRA_ID.TICKETS
            SET 
                DATE_PLANNED = DATEADD(MINUTE, 1, GETDATE())
            OUTPUT 
                INSERTED.ID,
                INSERTED.GUID,
                INSERTED.UUIDS,
                INSERTED.USERNAME,
                INSERTED.ENDPOINT,
                INSERTED.PAYLOAD,
                INSERTED.DESCRIPTION
            WHERE 
                ID = (
                    SELECT TOP 1 ID
                    FROM SCH_CENTRA_ID.TICKETS
                    WHERE STATUS = 'ABIERTO'
                    ORDER BY DATE_PLANNED
                )
