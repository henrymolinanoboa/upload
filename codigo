CREATE TABLE
    SCH_CENTRA_ID.TICKETS
    (
        ID BIGINT NOT NULL IDENTITY,
        GUID NVARCHAR(100) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        USERNAME NVARCHAR(50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        ENDPOINT NVARCHAR(200) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        PAYLOAD TEXT COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        STATUS NVARCHAR(20) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        DATE_CREATED DATETIME NOT NULL,
        DATE_PLANNED DATETIME,
        DATE_EXECUTE DATETIME,
        UUIDS TEXT COLLATE SQL_Latin1_General_CP1_CI_AS,
        DESCRIPTION TEXT COLLATE SQL_Latin1_General_CP1_CI_AS,
        CONSTRAINT TICKETS_PK PRIMARY KEY (ID)
    );


DROP TABLE
    SCH_CENTRA_ID.TICKETS_HISTORY;
CREATE TABLE
    SCH_CENTRA_ID.TICKETS_HISTORY
    (
        ID BIGINT NOT NULL IDENTITY,
        GUID NVARCHAR(100) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        DATE_EXECUTE DATETIME NOT NULL,
        RESPONSE TEXT COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,
        HTTP_RESPONSE NVARCHAR(100) COLLATE SQL_Latin1_General_CP1_CI_AS,
        CONSTRAINT TICKETS_HISTORY_PK PRIMARY KEY (ID)
    );
